<!--pages/nr1/1.wxml-->
<view class="box1">
<text>登录界面</text>
</view>

<view class="container"> 
 
 <!-- <view class="login-icon"> 
 <image class="login-img" src="../img/lcj2.jpg"></image> 
 </view>  -->
 <view class="login-from"> 
 
 <!--账号-->
 <view class="inputView"> 
  
  <label class="loginLab">账号</label> 
  <input class="inputText" placeholder="请输入账号" bindinput="usernameInput" /> 
 </view> 
 <view class="line"></view> 
 
 <!--密码-->
 <view class="inputView"> 
 
  <label class="loginLab">密码</label> 
  <input class="inputText" password="true" placeholder="请输入密码" bindinput="passwordInput" /> 
 </view> 
 
 <!--按钮-->
 <view class="loginBtnView">
  <button class="loginBtn" type="primary" size="{{primarySize}}" loading="{{loading}}"
   plain="{{plain}}" disabled="{{disabled}}" bindtap="login">登录</button> 
 </view>
  <button class="confirm"  bindtap="tz_register">注册</button>
 </view> 
</view>

